  "query": "\n            UPDATE address a\n            SET line_1 = $2,\n            line_2 = $3,\n            city = $4,\n            state = $5,\n            postal_code = $6,\n            country = $7\n            FROM user_profile up\n            WHERE up.address_id = a.id\n            AND up.user_id = $1\n            RETURNING a.id, line_1, line_2, city, state AS \"state:State\", postal_code, country, county, congressional_district, state_senate_district, state_house_district\n        ",
